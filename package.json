  "version": "0.0.1",
  "icon": "logo.png",
  "publisher": "Leila & Aime",
  "engines": {
    "vscode": "^1.64.0"
  },
  "keywords": [
    "JavaScript",
    "React",
    "Vue",
    "Appwrite",
    "Angular",
    "Svelte",
    "Dart",
    "Flutter",
    "Android",
    "react-native"
  ],
  "categories": [
    "Snippets"
  ],
  "homepage": "https://github.com/gasaroleila/appwrite-snippets",
  "repository": {
    "type": "git",
    "url": "https://github.com/gasaroleila/appwrite-snippets.git"
  },
  "scripts": {
    "deploy": "vsce publish --yarn"
  },
  "contributes": {
    "snippets": [
      {
