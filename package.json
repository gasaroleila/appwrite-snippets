        "path": "./snippets/client-sdks/web/client.code-snippets"
      },
      {
        "scope": "javascript,typescript,dart",
        "path": "./snippets/client-sdks/web/account.code-snippets"
      },
      {
        "scope": "javascript,typescript,dart",
        "path": "./snippets/client-sdks/web/database.code-snippets"
      },
      {
        "scope": "javascript,typescript,dart",
        "path": "./snippets/client-sdks/web/storage.code-snippets"
      },
      {
        "scope": "javascript,typescript,dart",
        "path": "./snippets/client-sdks/web/teams.code-snippets"
      },
      {
        "scope": "dart",
        "path": "./snippets/client-sdks/flutter/account.code-snippets"
      },
      {
        "scope": "dart",
        "path": "./snippets/client-sdks/flutter/client.code-snippets"
      },
      {
        "scope": "dart",
        "path": "./snippets/client-sdks/flutter/storage.code-snippets"
